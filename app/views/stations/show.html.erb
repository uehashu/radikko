<div>
  <h2><%= @station_name %></h2>
  
  <table class="table table-hover">
    <thead>
      <tr>
	<th>start_date</th>
	<th>end_date</th>
	<th>duration_sec</th>
	<th>title</th>
	<th>performers</th>
      </tr>
    </thead>

    <tbody>
      <% @programs.each do |program| %>
	<tr>
	  <td><%= program.start_date %></td>
	  <td><%= program.end_date %></td>
	  <td><%= program.duration_sec %></td>
	  <td><%= program.title %></td>
	  <td><%= program.performers %></td>
	</tr>
      <% end %>
    </tbody>
  </table>
</div>
